{
  "name": "najs-eloquent",
  "version": "0.2.20",
  "scripts": {
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc -d && ./script/copy-custom-definitions.js",
    "coverage": "export SUPPRESS_NO_CONFIG_WARNING=0 && npm run build && jest ./dist/test --coverage --forceExit",
    "test": "export SUPPRESS_NO_CONFIG_WARNING=0 && jest ./dist/test --forceExit",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prepublishOnly": "tsc -d -p ./ --outDir dist"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json",
        "skipBabel": true
      }
    },
    "moduleFileExtensions": ["js", "ts", "json"],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": ["**/test/**/*.test.ts", "**/test/**/*.test.js"],
    "testEnvironment": "node"
  },
  "files": ["dist/**/*"],
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/najs-framework/najs-eloquent.git"
  },
  "author": {
    "name": "Nhat Phan",
    "email": "nhat@ntworld.net"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.7"
  },
  "peerDependencies": {
    "collect.js": "^4.0.11",
    "moment": "^2.21.0",
    "najs-binding": "^1.4.0"
  },
  "dependencies": {
    "chance": "^1.0.13",
    "lodash": "^4.17.4",
    "mongoose": "^5.0.11",
    "mongoose-timestamps-moment": "^1.0.0",
    "najs-facade": "^1.1.1",
    "pluralize": "^7.0.0"
  },
  "devDependencies": {
    "@types/chance": "^1.0.0",
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.91",
    "@types/mongoose": "^4.7.31",
    "@types/pluralize": "^0.0.28",
    "@types/sinon": "^4.1.2",
    "collect.js": "^4.0.11",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1",
    "moment": "^2.21.0",
    "mongoose": "^5.0.11",
    "najs-binding": "^1.4.0",
    "sinon": "^4.1.3",
    "ts-jest": "^21.2.3",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
